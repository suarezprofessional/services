modules = ["web", "nodejs-20", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Website Server"

[[workflows.workflow]]
name = "Website Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 33009
externalPort = 3001

[[ports]]
localPort = 36465
externalPort = 3000

[[ports]]
localPort = 36711
externalPort = 80

[deployment]
deploymentTarget = "autoscale"
run = ["python3", "server.py"]
